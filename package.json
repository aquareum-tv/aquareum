{
  "name": "aquareum-monorepo",
  "private": true,
  "version": "0.0.0-monorepo",
  "description": "The video layer for everything.",
  "main": "index.js",
  "scripts": {
    "check": "yarn run check:workspaces && git ls-files | xargs prettier --check --ignore-unknown",
    "check:workspaces": "cd js/app && yarn run check",
    "fix": "git ls-files | xargs prettier --write --ignore-unknown",
    "postinstall": "husky",
    "build": "yarn workspaces foreach --all run build",
    "prepare": "husky",
    "release": "lerna publish --force-publish",
    "precommit": "make precommit",
    "app": "cd js/app && yarn run \"$@\"",
    "desktop": "cd js/desktop && yarn run \"$@\""
  },
  "keywords": [],
  "author": "Aquareum",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.1",
    "lerna": "^8.1.6",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3"
  },
  "workspaces": [
    "js/*"
  ],
  "packageManager": "yarn@4.3.0",
  "dependencies": {
    "firebase-admin": "^12.2.0"
  }
}
