{
  "name": "aquareum",
  "main": "expo-router/entry",
  "version": "0.0.2",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "npx expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web",
    "test": "jest --watchAll",
    "build": "expo export -p web || expo export -p web",
    "prepare": "rm -rf android/app/src/main/assets/fonts && mkdir -p ./android/app/src/main/assets/fonts && cp -R ./assets/fonts ./android/app/src/main/assets",
    "check": "npx tsc -p . --outFile $(mktemp)",
    "prebuild": "expo prebuild --clean && sed -i '' 's/org.gradle.jvmargs=-Xmx2048m -XX:MaxMetaspaceSize=512m/org.gradle.jvmargs=-Xmx8192m -XX:MaxMetaspaceSize=2048m/' android/gradle.properties && echo '\nnetworkTimeout=100000' >> android/gradle.properties",
    "postinstall": "cd ios && pod install && yarn run find-node",
    "find-node": "node -p '`NODE_BINARY=${process.argv[0]}`' > ios/.xcode.env.local"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/messaging": "^20.1.0",
    "@react-navigation/native": "^6.1.17",
    "@tamagui/config": "^1.100.6",
    "@tamagui/lucide-icons": "^1.100.6",
    "@tamagui/toast": "^1.100.6",
    "babel-preset-expo": "^11.0.6",
    "burnt": "^0.12.2",
    "chrono-node": "^2.7.6",
    "expo": "~51.0.9",
    "expo-build-properties": "^0.12.3",
    "expo-font": "~12.0.6",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.9",
    "expo-router": "~3.5.15",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "^1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.17",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "react-native-webview": "13.8.6",
    "tamagui": "^1.100.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@expo/metro-runtime": "~3.2.1",
    "@tamagui/babel-plugin": "^1.100.6",
    "@tamagui/metro-plugin": "^1.100.6",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
